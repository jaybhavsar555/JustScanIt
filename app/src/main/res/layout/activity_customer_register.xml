<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CustomerRegisterActivity"
    android:background="#494dcd">


  <EditText
      android:id="@+id/Name"
      android:layout_width="370dp"
      android:layout_height="60dp"
      android:autofillHints=""
      android:background="@drawable/edittext_round"
      android:ems="10"
      android:hint="@string/name"
      android:inputType="text"
      android:padding="10dp"
      android:paddingStart="15dp"
      android:paddingEnd="2dp"
      android:textColor="@color/black"
      android:textColorHint="@color/black"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.487"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.193" />

  <EditText
      android:id="@+id/EmailID"
      android:layout_width="370dp"
      android:layout_height="60dp"
      android:layout_marginBottom="452dp"
      android:autofillHints=""
      android:background="@drawable/edittext_round"
      android:ems="10"
      android:hint="@string/enter_emailid"
      android:inputType="textEmailAddress"
      android:padding="10dp"
      android:paddingStart="15dp"
      android:paddingEnd="3dp"
      android:textColor="#000000"
      android:textColorHint="@color/black"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.39"
      app:layout_constraintStart_toStartOf="parent" />

  <com.google.android.material.textfield.TextInputLayout
      android:id="@+id/textInputLayout3"
      android:layout_width="370dp"
      android:layout_height="60dp"
      android:background="@drawable/edittext_round"
      android:textColorHint="@color/black"
      app:errorTextColor="@color/black"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.424"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.458"
      app:passwordToggleEnabled="true">

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/Password"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/edittext_round"
        android:hint="@string/enter_password"
        android:inputType="textPassword"
        android:paddingStart="15dp"
        android:paddingEnd="3dp"
        android:textColor="@color/black"
        android:textColorHint="@color/black" />

  </com.google.android.material.textfield.TextInputLayout>

  <com.google.android.material.textfield.TextInputLayout
      android:layout_width="370dp"
      android:layout_height="60dp"
      android:background="@drawable/edittext_round"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.45"
      android:textColorHint="@color/black"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.574"
      app:passwordToggleEnabled="true">

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/ConfirmPassword"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/edittext_round"
        android:ems="10"
        android:paddingStart="15dp"
        android:paddingEnd="3dp"
        android:textColor="@color/black"
        android:padding="10dp"
        android:textColorHint="@color/black"
        android:hint="@string/confirm_password"
        android:importantForAutofill="no"
        android:inputType="textPassword" />


  </com.google.android.material.textfield.TextInputLayout>

  <EditText
      android:id="@+id/PhoneNumber"
      android:layout_width="370dp"
      android:layout_height="60dp"
      android:autofillHints=""
      android:ems="10"
      android:hint="@string/phone_number"
      android:paddingStart="15dp"
      android:paddingEnd="3dp"
      android:inputType="phone"
      android:padding="10dp"
      android:textColorHint="@color/black"
      android:textColor="#090909"
      android:background="@drawable/edittext_round"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.371"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.689" />

  <androidx.appcompat.widget.AppCompatButton
      android:id="@+id/btnRegister"
      android:layout_width="364dp"
      android:layout_height="52dp"
      android:background="@drawable/button_round1"
      android:text="@string/register"
      android:textColor="@color/white"
      android:textSize="18sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.34"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.796" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/already_have_an_account"
      android:textColor="#ff927b"
      android:textSize="15sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.076"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btnRegister"
      app:layout_constraintVertical_bias="0.243" />

  <TextView
      android:id="@+id/login"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/logIn"
      android:textColor="@color/white"
      android:textSize="15sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.545"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.874" />

  <TextView
      android:id="@+id/textView11"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/customer_registration"
      android:textSize="25sp"
      android:textStyle="bold"
      android:textColor="@color/orange"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.46"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.073" />


</androidx.constraintlayout.widget.ConstraintLayout>